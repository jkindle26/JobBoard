@model IEnumerable<IdentitySample.Models.ApplicationUser>

@{
    ViewBag.Title = "Index";
}
<section class="masthead team" style="padding:10rem 5rem ;">

    <h2 class="text-white" style="font-size:4em;">Users</h2>

    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table-striped text-white" style="border-spacing:15px;">
        <tr>
            <th>
                User Emails
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.UserName)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                </td>
            </tr>
        }

    </table>
</section>